{
  "manifest_version": 2,
  "name": "Name Entity Recognition extension",
  "version": "1.0",
  "author": "Trungpy",
  "description": "An extension scan the name entity of a webpage.",
  "permissions": [
    "storage", 
    "activeTab", 
    "tabs",
    "notifications",
    "http://127.0.0.1:8000/",
    "https://en.wikipedia.org/",
    "https://medium.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "icons":{
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}



